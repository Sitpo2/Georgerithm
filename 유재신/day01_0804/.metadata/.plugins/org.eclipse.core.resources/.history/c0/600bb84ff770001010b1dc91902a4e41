package SEWA_2068;

import java.io.File;
import java.util.Arrays;
import java.util.Scanner;

public class Max_Number {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		
		//처음에 # 적용하기
		
		int T;
		T = sc.nextInt();
		
		for(int C = 1; C<=T; C++) {
		
		//배열정의
		int [] arr = new int [10];
		int N = arr.length;
		System.out.println(arr.length);
		
		
		//배열 입력하기
		for(int i =0; i<N; i++) { //왜 10해야 되지? N이 안되는 이유는?
			arr[i] = sc.nextInt();
		}
		
		
		//최대값 구할때(앞의수와 뒤의수 반복적으로 해서 가장 큰 수 나오게 하기)
		for(int i = N-1; i>0; i--) {
			
						
			for(int j=0; j<i; j++) {
				//오름차순
				if(arr[j] > arr[j+1]) {					
					int tem = arr[j];
					arr[j] = arr[j+1];
					arr[j+1] = tem;					
				}
			}
			
		}	
		
			System.out.println(" ");
			System.out.print("#" + C + " " +  arr[N-1]);				
		}
					
	}


	}

