package SEWA_1984;

import java.util.Scanner;

public class Mid_Number {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int t;
		t = sc.nextInt();
		//# 반복하는 것
		for(int C = 1; C<=t; C++) {
			
			//배열정의!
			int [] arr = new int [10];
			int N = arr.length;
			//System.out.println(N);
			
			//배열 입력하기
			for(int i =0; i<N; i++) { 
				arr[i] = sc.nextInt();
			}
			
			//오름차순 진행방법
			for(int i = N-1; i>=0; i--) {
				for(int j = 0; j<i; j++) {
					if(arr[j]>arr[j+1]) {
						int tmp = arr[j];
						arr[j] = arr[j+1];
						arr[j+1] = tmp;
					}
						
				}
			}
						
			//최대값, 최소값 빼고 평균구하기
			int sum = 0;
			for(int k = 1; k<=N-2; k++) {
				sum = sum + arr[k];
								
			}
			
			int avg = sum/(N-2);
			
			System.out.println("#" + C + " " +  avg);
						
		}
	}
}
