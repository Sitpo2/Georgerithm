package SEWA_1984;

import java.util.Scanner;
import java.util.Arrays;

public class Mid_Number {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int T;
		T = sc.nextInt();
		//# 반복하는 것
		for(int C = 1; C<=T; C++) {
			
			//배열정의!
			int [] arr = new int [10];
			int N = arr.length;
			//System.out.println(N);
			
			//배열 입력하기
			for(int i =0; i<N; i++) { //왜 10해야 되지? N이 안되는 이유는?
				arr[i] = sc.nextInt();
			}
			
			//오름차순 진행방법
			for(int i = N-1; i>=0; i--) {
				for(int j = 0; j<i; j++) {
					if(arr[j]>arr[j+1]) {
						int tmp = arr[j];
						arr[j] = arr[j+1];
						arr[j+1] = tmp;
					}
						
				}
			}
			
			System.out.println(" ");
			System.out.print("#" + C + " " +  arr[N/2]);
			
		}

	}

}
