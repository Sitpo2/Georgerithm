package SEWA_2068;

import java.util.Arrays;
import java.util.Scanner;

public class Max_Number2 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		// # 관련되서 풀기 위해 입력
		int T;
		T = sc.nextInt();
		
		for(int C = 1; C<=T; C++) {
			
			//배열 정의
			int [] arr = new int [10];
			//배열 길이 정의
			int N = arr.length;
			
			for(int i =0; i<N; i++) {
				arr[i] = sc.nextInt();
			}
			
			Arrays.sort(arr);
			System.out.println(arr[N-1]);
					
		}
	}

}
